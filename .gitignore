# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep
.DS_store

# Ignore Byebug command history file.
.byebug_history
.env
dump.rdb
latest.dump
latest.dump.1
latest.dump.2
latest.dump.3
